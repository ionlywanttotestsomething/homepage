{
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 120,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.sh, .husky/pre-commit",
      "options": {
        "endOfLine": "lf"
      }
    },
    {
      "files": "*.bat",
      "options": {
        "endOfLine": "crlf"
      }
    },
    {
      "files": ".eslintcache",
      "options": {
        "parser": "json"
      }
    }
  ]
}
