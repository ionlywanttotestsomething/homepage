    "prebuild:updatenv": "",
    "build:prod": "ng build --configuration production",
    "build:github": "ng build --base-href /homepage/ --configuration production",
    "prettier": "prettier .",
    "prettier:check": "npm run prettier -- --check",
    "prettier:format": "npm run prettier -- --write",
